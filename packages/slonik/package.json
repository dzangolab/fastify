  },
  "devDependencies": {
    "@12deg/eslint-config": "0.1.8",
    "@12deg/tsconfig": "0.1.4",
    "@dzangolab/fastify-config": "0.75.2",
    "@slonik/driver": "46.1.0",
    "@types/humps": "2.0.6",
    "@types/node": "20.16.14",
    "@types/pg": "8.11.9",
    "@vitest/coverage-istanbul": "2.0.5",
    "eslint": "8.57.1",
    "fastify": "4.10.2",
    "fastify-plugin": "4.5.0",
    "pg-mem": "3.0.2",
    "prettier": "3.3.3",
    "slonik": "46.1.0",
    "typescript": "5.5.4",
    "vite": "5.3.6",
    "vitest": "2.0.5",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "@dzangolab/fastify-config": "0.75.2",
    "fastify": ">=4.10.2",
    "fastify-plugin": ">=4.5.0",
    "pg-mem": ">=3.0.2",
    "slonik": ">=46.1.0",
    "zod": ">=3.23.8"
