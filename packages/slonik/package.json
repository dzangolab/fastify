{
  "name": "@dzangolab/fastify-slonik",
  "version": "0.73.1",
  "description": "Fastify slonik plugin",
  "homepage": "https://github.com/dzangolab/fastify/tree/main/packages/slonik#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzangolab/fastify.git",
    "directory": "packages/slonik"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dzangolab-fastify-slonik.js",
      "require": "./dist/dzangolab-fastify-slonik.umd.cjs"
    }
  },
  "main": "./dist/dzangolab-fastify-slonik.umd.cjs",
  "module": "./dist/dzangolab-fastify-slonik.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "sort-package": "npx sort-package-json",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@dzangolab/postgres-migrations": "5.4.2",
    "humps": "2.0.1",
    "pg": "8.12.0"
  },
  "devDependencies": {
    "@dzangolab/fastify-config": "workspace:*",
    "@types/humps": "2.0.6",
    "@types/node": "20.16.5",
    "@types/pg": "8.11.9",
    "@vitest/coverage-istanbul": "2.0.5",
    "eslint": "9.11.1",
    "eslint-config-custom": "workspace:*",
    "fastify": "4.10.2",
    "fastify-plugin": "4.5.0",
    "prettier": "3.3.3",
    "slonik": "38.0.0",
    "tsconfig": "workspace:*",
    "typescript": "4.9.5",
    "vite": "4.5.3",
    "vitest": "2.0.5",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "@dzangolab/fastify-config": "workspace:*",
    "fastify": ">=4.10.2",
    "fastify-plugin": ">=4.5.0",
    "slonik": ">=38.0.0",
    "zod": ">=3.23.8"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}