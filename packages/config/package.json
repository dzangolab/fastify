{
  "name": "@dzangolab/fastify-config",
  "version": "0.73.1",
  "description": "Fastify config plugin",
  "homepage": "https://github.com/dzangolab/fastify/tree/main/packages/config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzangolab/fastify.git",
    "directory": "packages/config"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dzangolab-fastify-config.js",
      "require": "./dist/dzangolab-fastify-config.umd.cjs"
    }
  },
  "main": "./dist/dzangolab-fastify-config.umd.cjs",
  "module": "./dist/dzangolab-fastify-config.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "sort-package": "npx sort-package-json",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "devDependencies": {
    "@types/node": "20.16.5",
    "@vitest/coverage-istanbul": "2.0.5",
    "eslint": "9.11.1",
    "eslint-config-custom": "workspace:*",
    "fastify": "4.10.2",
    "fastify-plugin": "4.5.0",
    "pino": "8.21.0",
    "prettier": "3.3.3",
    "tsconfig": "workspace:*",
    "typescript": "4.9.5",
    "vite": "4.5.3",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "fastify": ">=4.10.2",
    "fastify-plugin": ">=4.5.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}