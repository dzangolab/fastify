{
  "name": "@dzangolab/fastify-mailer",
  "version": "0.17.1",
  "description": "Fastify mailer plugin",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dzangolab-fastify-mailer.js",
      "require": "./dist/dzangolab-fastify-mailer.umd.cjs"
    }
  },
  "main": "./dist/dzangolab-fastify-mailer.umd.cjs",
  "module": "./dist/dzangolab-fastify-mailer.js",
  "types": "./dist/types/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "sort-package": "npx sort-package-json",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "html-minifier": "4.0.0",
    "html-to-text": "9.0.3",
    "mustache": "4.2.0",
    "nodemailer": "6.9.1",
    "nodemailer-html-to-text": "3.2.0",
    "nodemailer-mjml": "1.2.4"
  },
  "devDependencies": {
    "@dzangolab/fastify-config": "0.17.1",
    "@types/mjml": "4.7.0",
    "@types/node": "18.11.19",
    "@types/nodemailer": "6.4.7",
    "@types/nodemailer-html-to-text": "3.1.0",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint": "8.33.0",
    "eslint-config-custom": "0.17.1",
    "fastify": "4.10.2",
    "fastify-plugin": "4.4.0",
    "mjml": "4.13.0",
    "prettier": "2.8.3",
    "tsconfig": "0.17.1",
    "typescript": "4.9.5",
    "vite": "4.1.1"
  },
  "peerDependencies": {
    "@dzangolab/fastify-config": "0.17.1",
    "fastify": ">=4.9.2",
    "fastify-plugin": ">=4.3.0",
    "mjml": ">=4.13.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
