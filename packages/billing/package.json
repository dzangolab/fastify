{
  "name": "@12deg/billing-fastify",
  "version": "0.1.0",
  "description": "billing plugin for fastify",
  "homepage": "https://github.com/12deg/billing/tree/main/packages/fastify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/12deg/billing.git",
    "directory": "packages/fastify"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/12deg-billing-fastify.js",
      "require": "./dist/12deg-billing-fastify.umd.cjs"
    }
  },
  "main": "./dist/12deg-billing-fastify.umd.cjs",
  "module": "./dist/12deg-billing-fastify.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "sort-package": "npx sort-package-json",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@graphql-tools/merge": "9.0.7",
    "graphql": "16.9.0",
    "graphql-tag": "2.12.6"
  },
  "devDependencies": {
    "@dzangolab/fastify-config": "0.71.3",
    "@dzangolab/fastify-graphql": "0.71.3",
    "@dzangolab/fastify-slonik": "0.71.3",
    "@types/node": "20.12.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-istanbul": "2.0.4",
    "eslint": "8.56.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "46.0.1",
    "fastify": "4.10.2",
    "fastify-plugin": "4.5.0",
    "prettier": "2.8.8",
    "slonik": "38.0.0",
    "supertokens-node": "14.1.3",
    "typescript": "4.9.5",
    "vite": "4.5.3",
    "vitest": "2.0.4",
    "mercurius": "14.1.0",
    "zod": "3.23.8"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "peerDependencies": {
    "@dzangolab/fastify-config": ">=0.71.3",
    "@dzangolab/fastify-graphql": ">=0.71.3",
    "@dzangolab/fastify-slonik": ">=0.71.3",
    "mercurius": ">=14.1.0",
    "fastify": ">=4.10.2",
    "fastify-plugin": ">=4.5.0",
    "slonik": ">=38.0.0",
    "supertokens-node": ">=14.1.3",
    "zod": ">=3.23.8"
  }
}
